<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="view_suppliers_logger_tree" model="ir.ui.view">
        <field name="name">suppliers.logger.tree</field>
        <field name="model">suppliers.logger</field>
        <field name="arch" type="xml">
            <tree string="Suppliers List">
                <field name="supplier_origin"/>
                <!-- <field name="test_id"/> -->
            </tree>
        </field>
    </record>

    <record id="view_shelves_logger_form" model="ir.ui.view">
        <field name="name">suppliers.logger.form</field>
        <field name="model">suppliers.logger</field>
        <field name="arch" type="xml">
            <form string="Supplier Details">
                <sheet>
                    <group>
                        <field name="supplier_origin"/>
                        <!-- <field name="test_id"/> -->
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="action_suppliers_logger" model="ir.actions.act_window">
        <field name="name">Suppliers</field>
        <field name="res_model">suppliers.logger</field>
        <field name="view_mode">tree,form</field>
    </record>


    <menuitem   id="menu_suppliers_logger" parent ="menu_logger"
                action="action_suppliers_logger" name='Suppliers'
                sequence='5'/>
    
</odoo>