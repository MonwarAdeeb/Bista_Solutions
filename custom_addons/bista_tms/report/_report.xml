<?xml version="1.0" encoding="UTF-8"?>

<odoo>

    <record id="_report" model="ir.actions.report">
        <field name="name"></field>
        <field name="model">Model Name</field>
        <field name="report_type">qweb-html</field>
        <field name="report_name">modulename.something_report_document</field>
        <field name="report_file">modulename.something_report_document<</field>
        <field name="binding_model_id" ref="model_modelName"/>
        <field name="binding_type">report</field>        
    </record>

    <template id="something_report_document">
        <t t-call="web.html_container">
            <t t-call="web.external_layout">
                <div class="page">
                    <t t-foreach='docs' t-as='doc'>
                        
                        
                    </t>
                </div>

            </t>
        </t>
    </template>

</odoo>